{
  "name": "z",
  "version": "0.0.7",
  "main": "src/z.js",
  "description": "pattern matching for javascript",
  "scripts": {
    "test": "mocha tests/*.js",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-pattern-matching/z"
  },
  "author": "Wagner Leonardi",
  "keywords": [
    "pattern",
    "matching",
    "functional",
    "haskell"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  },
  "nyc": {
    "statements": 90,
    "branches": 75,
    "functions": 90,
    "lines": 90,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*",
      "tests/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/z-pattern-matching/z.git./issues"
  },
  "homepage": "https://github.com/z-pattern-matching/z.git.#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.4.0",
    "coveralls": "^2.11.16",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^10.1.2"
  },
  "engines": {
    "node": "< 6.0.0"
  }
}
